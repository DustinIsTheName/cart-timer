<style id="wittyCartFloatStyles" type="text/css">

	#wittyCartFloatTimer {
		position: fixed;
		<%= o.float_position %>
		background: <%= o.float_bg_color %>;
		color: <%= o.float_text_color %>;
		border: 1px solid <%= o.float_border %>;
		border-radius: <%= o.float_border_radius%>px;
		font-size: <%= o.float_font_size %>px;
	}

	#wittyCartFloatTimer .wittyCountDownTimer {
		color: <%= o.float_timer_color %>;
		font-size: <%= o.float_timer_size%>px;
	}

</style>