<style id="wittyCartQuickStyles" type="text/css">

  #wittyCartLowBarTimer {
  	background: <%= o.low_time_bar_bg_color %>;
	  color: <%= o.low_time_bar_text_color %>;
	  border: 1px solid <%= o.low_time_bar_border %>;
	  border-radius: <%= o.low_time_bar_border_radius %>px;
	  font-size: <%= o.low_time_bar_font_size %>px;
	}

  #wittyCartLowBarTimer .wittyCountDownTimerHolder {
  	border-right: 2px solid <%= o.low_time_bar_border %>;
  }

  #wittyCartLowBarTimer .wittyCountDownTimer {
  	font-size: <%= o.low_time_bar_timer_size %>px;
	  color: <%= o.low_time_bar_timer_color %>;
	}

  #wittyCartLowBarTimer #wittyButtonLowCart {
  	background: <%= o.low_time_view_cart_bg_color %>;
	  color: <%= o.low_time_view_cart_text_color %>;
	  border: 1px solid  <%= o.low_time_view_cart_border %>;
	  border-radius: <%= o.low_time_view_cart_border_radius %>px;
	}

  #wittyCartLowBarTimer #wittyButtonLowCheckout {
  	background: <%= o.low_time_checkout_bg_color %>;
	  color: <%= +o.low_time_checkout_text_color %>;
	  border: 1px solid  <%= o.low_time_checkout_border %>;
	  border-radius: <%= o.low_time_checkout_border_radius %>px;
	}

</style>